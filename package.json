{
  "name": "limago-reflexo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production && npm run purgecss",
    "purgecss": "purgecss --config purgecss.config.js --output ./dist/limago-reflexo/",
    "watch": "ng build --watch --configuration=development",
    "test": "ng test",
    "build:ssr": "ng build --configuration=production && ng run limago-reflexo:server:production",
    "serve:ssr": "node dist/limago-reflexo/server/main.js",
    "dev:ssr": "ng run limago-reflexo:serve-ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.1.0",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "bootstrap": "^5.3.3",
    "express": "^4.18.2",
    "ngx-bootstrap": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.0",
    "@angular/cli": "^18.1.0",
    "@angular/compiler-cli": "^18.1.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/node": "^18.18.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "purgecss": "^6.0.0",
    "typescript": "5.4",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
